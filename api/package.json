{
  "name": "blogs-api",
  "version": "0.1.0",
  "engines": {
    "node": ">=16.0.0"
  },
  "engineStrict": true,
  "private": true,
  "dependencies": {
    "dotenv": "^16.3.1",
    "express": "4.18.2",
    "express-async-errors": "3.1.1",
    "joi": "17.6.0",
    "jsonwebtoken": "9.0.0",
    "morgan": "1.10.0",
    "mysql2": "2.1.0",
    "railway": "^2.0.17",
    "sequelize": "6.30.0",
    "shelljs": "0.8.5"
  },
  "main": "src/server.js",
  "scripts": {
    "start": "node .",
    "drop": "npx sequelize-cli db:drop",
    "seed": "npx sequelize-cli db:seed:all",
    "test-coverage": "jest -i --verbose --forceExit --coverage",
    "predev": "npx sequelize-cli db:create && npx sequelize-cli db:migrate",
    "dev": "nodemon .",
    "lint": "eslint --no-inline-config --no-error-on-unmatched-pattern -c .eslintrc.json ."
  },
  "devDependencies": {
    "chai": "4.3.4",
    "chai-http": "4.3.0",
    "change-case": "4.1.2",
    "eslint-config-trybe-backend": "2.1.0",
    "frisby": "2.1.2",
    "jest": "26.0.1",
    "jest-each": "28.0.1",
    "mocha": "8.4.0",
    "nodemon": "2.0.22",
    "nyc": "15.1.0",
    "sequelize-cli": "6.2.0",
    "sequelize-jest-helpers-trybe": "1.0.0",
    "sinon": "11.1.1",
    "sinon-chai": "3.7.0",
    "supertest": "4.0.2",
    "wait-on": "6.0.1"
  }
}
